FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 293,15;

boundaryField
{
    floor
    {
        type            zeroGradient;
    }

    ceiling
    {
		type            zeroGradient;
    }

    fixedWalls
    {
        type            zeroGradient;
    }
	
	inlet
    {
        type            fixedValue;
        value           uniform 293,15;
    }
	
	outlet
    {
		type            zeroGradient;
    }
}

